{
  "name": "@nosana/kit",
  "version": "0.0.0",
  "description": "Nosana KIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "generate-clients": "npx tsx scripts/generate-clients.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check \"**/*.ts\"",
    "format:fix": "prettier --write \"**/*.ts\"",
    "publish:public": "npm run build && npm publish --access public"
  },
  "keywords": [
    "nosana",
    "solana",
    "sdk",
    "kit",
    "blockchain"
  ],
  "author": "Nosana",
  "license": "MIT",
  "dependencies": {
    "@solana-program/token": "0.5.1",
    "@solana/sysvars": "5.0.0",
    "axios": "1.6.0",
    "bs58": "6.0.0",
    "buffer": "6.0.3",
    "form-data": "4.0.0",
    "gill": "0.12.0"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.2.0",
    "@codama/renderers-js": "^1.2.14",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^1.6.0",
    "codama": "^1.3.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.18.0"
  }
}
